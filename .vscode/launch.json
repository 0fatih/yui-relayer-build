      "args": ["tx", "update-clients", "ibc01", "--home=${workspaceFolder}/tests/cases/eth2eth/demo/.urelayer", "--debug"]
    },
    {
      "name": "TM2ETH - Create Client",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/relayer/main.go",
      "args": ["tx", "clients", "ibc01", "--debug"]
    },
    {
      "name": "TM2ETH - Update Clients",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/relayer/main.go",
      "args": ["tx", "update-clients", "ibc01", "--debug"]
    },
    {
