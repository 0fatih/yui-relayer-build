  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/relayer/main.go",
      "args": ["start"]
    },
    {
      "name": "ETH2ETH - Create Client",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/relayer/main.go",
      "args": ["tx", "clients", "ibc01", "--home=${workspaceFolder}/tests/cases/eth2eth/demo/.urelayer", "--debug"]
    },
    {
      "name": "ETH2ETH - Update Clients",
      "type": "go",
      "request": "launch",
      "mode": "auto",
